      "source": [
        "n = int(input(\"enter a number: \"))\n",
        "i = 2\n",
        "flag = 0\n",
        "while(i<=n/2):\n",
        "  if n % i == 0:\n",
        "     flag = 1\n",
        "     break\n",
        "  i += 1\n",
        "if flag == 1:\n",
        "  print(\"not a prime number\")\n",
        "else:\n",
        "  print(\"prime number\")"
      ]
